{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Dev",
      "type": "node-terminal",
      "request": "launch",
      "command": "yarn run dev"
    },
    {
      "name": "Run Dev Locally",
      "type": "node-terminal",
      "request": "launch",
      "command": "yarn run dev:local"
    },
    {
      "name": "Launch Yonda Cloud",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["workspace", "@Yonda/server", "dev"]
    }
  ]
}
